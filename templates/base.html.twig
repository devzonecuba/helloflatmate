                                    </div>
                                </div>
                            </form>
                        </div>

                        <div>

                        </div>

                    </div>

                </div>
            </div>
        </div>

        <div class="click-closed"></div>
        <!--/ Form Search Star /-->
        <div class="box-collapse">
            <div class="title-box-d">
                <h3 class="title-d">Buscar</h3>
            </div>
            <span class="close-box-collapse right-boxed ion-ios-close"></span>
            <div class="box-collapse-wrap form">
                <form class="form-a">
                    <div class="row">

                        {#Universidad#}
                        <div class="col-md-6 mb-2">
                            <div class="form-group">
                                <label for="Type">Universidad</label>
                                <select class="form-control form-control-lg form-control-a" id="Type">
                                    <option>Universidad</option>
                                    <option>ESIC</option>
                                    <option>ESAT</option>
                                    <option>UCH-CEU</option>
                                    <option>UEM</option>
                                    <option>UPV</option>
                                    <option>UPV</option>
                                    <option>Universidad de Valencia</option>
                                    <option>Universidad Catolica</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="form-group">
                                <label for="city">Ciudad</label>
                                <select class="form-control form-control-lg form-control-a" id="city">
                                    <option>Benimacle</option>
                                    <option>Blasco Ibañez</option>
                                    <option>Centro</option>
                                    <option>Edificio Erasmus</option>
                                    <option>FDO Catolico</option>
                                    <option>Guimera</option>
                                    <option>Mocada</option>
                                </select>
                            </div>
                        </div>

                        <div class="col-md-6 mb-2">
                            <div class="form-group">
                                <label for="price">Precio Mínimo</label>
                                <input class="form-control form-control-lg form-control-a" type="text" placeholder="Precio mínimo">
                            </div>
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="form-group">
                                <label for="price">Precio Máximo</label>
                                <input class="form-control form-control-lg form-control-a" type="text" placeholder="Precio máximo">
                            </div>
                        </div>

                        <div class="col-md-6 mb-2">
                            <div class="form-group">
                                <label for="price">Fecha de Entrada</label>
                                <input class="form-control form-control-lg form-control-a" type="text" placeholder="Fecha entrada">
                            </div>
                        </div>
                        <div class="col-md-6 mb-2">
                            <div class="form-group">
                                <label for="price">Fecha de Salida</label>
                                <input class="form-control form-control-lg form-control-a" type="text" placeholder="Fecha salida">
                            </div>
                        </div>
                        <div class="col-md-12">
                            <button type="submit" class="btn btn-serch">Buscar</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
